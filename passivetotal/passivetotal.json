        "description": "Validate the asset configuration for connectivity.",
        "type": "test",
        "identifier": "test_asset_connectivity",
        "read_only": true,
        "parameters": {},
        "output": [],
        "versions": "EQ(*)"
    }, {
        "action": "get ip info",
        "description": "Queries PassiveTotal for IP-based enrichment calls.",
        "type": "investigate",
        "identifier": "get_ip_info",
        "read_only": true,
        "parameters": {
            "query": {
                "description": "Query value to use",
                "data_type": "string",
                "order": 0,
                "contains": ["ip"],
                "primary": true,
                "required": true
            },
            "task": {
                "data_type": "string",
                "order": 4,
                "description": "Task",
                "default": "metadata_ip",
                "required": true,
                "value_list": ["metadata_ip", "passive_dns", "ssl_certificate_history"]
            }
        },
        "render": {
            "type": "custom",
            "width": 12,
            "title": "IP Information",
            "height": 5,
            "view": "view_render.get_ip_info"
        },
        "output": [{
            "data_path": "action_result.status",
            "data_type": "string"
        }, {
            "data_path": "action_result.parameter.query",
            "data_type": "string",
            "contains": ["domain", "ip"],
            "column_name": "Query",
            "column_order": 0
        }],
        "versions": "EQ(*)"
    }, {
