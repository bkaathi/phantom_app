    "configuration": {
        "username": {
            "data_type": "string",
            "order": 0,
            "description": "PassiveTotal username (email)",
            "required": true
        },
        "api_key": {
            "data_type": "string",
            "order": 1,
            "description": "PassiveTotal API key",
            "required": true
        },
        "verify_server_cert": {
            "data_type": "boolean",
            "description": "Verify server certificate",
            "required": true,
            "order": 1,
            "default": false
        }
    },
    "actions": [{
